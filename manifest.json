{
  "name": "CLickAMe",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "scripting", "activeTab"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "/public/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/scripts/content.js"]
    }
  ]
}
